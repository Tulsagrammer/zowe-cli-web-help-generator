    {
      "name": "activate",
      "aliases": [
        "act"
      ],
      "summary": "Activate a setting or configuration",
      "description": "Activate a setting or configuration from stored configuration data",
      "type": "group",
      "children": [
        {
          "name": "policy",
          "aliases": [
            "pol"
          ],
          "summary": "Activates (refreshes) configuration (policy)",
          "description": "Updates all SAPlex members (Automation Manager and -Agents) with the definition that is stored in the specified policy dataset. \n Using '*' as policyName causes a refresh of current policy. \n                        \nRequires (min.): SA Operations API 1.1.0 \n                        \nFollowing Success Messages can be returned by this command:                         \nSACLI001100 - Policy activation with {PolicyDatasetName} has been requested                         \nSACLI001110 - Re-Activation of current active policy has been requested",
          "type": "command",
          "handler": "",
          "profile": {
            "optional": [
              "sa",
              "base"
            ]
          },
          "options": [
            {
              "name": "policyName",
              "type": "string",
              "description": "The name of the dataset that contains the policy",
              "group": "Options",
              "aliases": []
            },
            {
              "name": "reactivate",
              "aliases": [
                "*"
              ],
              "description": "Activate current active policy again",
              "type": "boolean",
              "group": "Options"
            },
            {
              "name": "suspendFileOption",
              "aliases": [
                "susp"
              ],
              "description": "Enables the suspend file option",
              "type": "string",
              "allowableValues": {
                "values": [
                  "NOSUSPEND",
                  "FORCESUSPEND "
                ],
                "caseSensitive": false
              },
              "required": false,
              "defaultValue": "NOSUSPEND",
